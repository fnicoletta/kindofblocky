{
	"schemaVersion": 1,
	"id": "kind-of-blocky",
	"version": "${version}",
	"name": "Kind of Blocky",
	"description": "a collection of minecraft materials and items",
	"authors": [
		"KindOfGuilty!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/kind-of-blocky/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "kindofguilty.kindofblocky.KindOfBlocky",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "kindofguilty.kindofblocky.KindOfBlockyDataGenerator",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "kindofguilty.kindofblocky.KindOfBlockyClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"kind-of-blocky.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.4",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}